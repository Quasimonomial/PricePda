<h1>Change your price range</h1>

	<label>
		First Name: <input type="text" name="user[password]">
	</label><br>
	<label>
		Last Name: <input type="text" name="user[password]">
	</label><br>
	<label>
		Hospital Name: <input type="text" name="user[password]">
	</label><br>
	<label>
		State: <input type="text" name="user[state]">
	</label><br>
	<label>
		Zip: <input type="text" name="user[zip_code]">
	</label><br>
	<label>
		Phone Number: <input type="text" name="user[phone]">
	</label><br>


<form action="<%= user_url(current_user)%>" method="POST">
	<input type="hidden" name="_method" value="PATCH">	
	<label>
		Range Percentage: <input type="text" name="user[price_range_percentage]", value="<%=current_user.price_range_percentage%>">
	</label>
	<input type="submit" value="Update Price Value">
</form>